{
  "name": "docforge-handbook-theme",
  "version": "0.0.15",
  "license": "MIT",
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack --config node_modules/webpack-mix/setup/webpack.config.js --watch --progress --hide-modules",
    "build": "cross-env NODE_ENV=production webpack --config node_modules/webpack-mix/setup/webpack.config.js --progress --hide-modules",
    "serve": "php -S localhost:8080 -t public ../serve.php",
    "start": "run-p serve watch"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.6.1",
    "cross-env": "^5.2.0",
    "jquery": "^3.5.0",
    "node-normalize-scss": "^8.0.0",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.15.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-mix": "^3.0.0"
  }
}
